# Nome exibido na interface do projeto
VITE_APP_NAME=Quantum Jud

# Base path utilizado pelo roteador quando o app é servido em um subdiretório
VITE_APP_BASE_PATH=/

# Caminho do painel administrativo
VITE_ADMIN_BASE_PATH=/admin

# URL base da API utilizada em produção
VITE_API_BASE_URL=https://api.example.com

# Habilita dados mockados enquanto a API oficial não estiver disponível
VITE_ENABLE_MOCKS=true
